{
    "apiVersion": "v1",
    "kind": "Service",
    "metadata": {
        "name": "my-service",
        "namespace": "default"
    },
    "spec": {
        "selector": {
            "apps": "my-app"
        },
        "ports": [
            {
                "protocol": "TCP",
                "port": 80,
                "targetPort": 9376
            }
        ]
    }
}